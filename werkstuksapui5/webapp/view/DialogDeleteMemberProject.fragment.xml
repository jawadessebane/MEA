<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.sap.build.standard.mobileEnterpriseProject.controller.DialogDeleteMemberProject" xmlns="sap.m">
    <Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Delete member" type="Standard" verticalScrolling="true">
        <content>
            	<ComboBox id="member"  items="{ path: '/MembersInfoSet' }">
                						<core:Item key="{Memberid}" text=" {Naam}"/>
        							 </ComboBox>
        </content>
        <beginButton/>
        <buttons>
            <Button text="Delete" press="_ondelete" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
            <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>
